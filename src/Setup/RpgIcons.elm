import Mobster.RpgRole exposing (..)


mobsterIcon : RpgRole -> Svg msg
mobsterIcon role =
    case role of
        Driver ->
            driverIcon

        Navigator ->
            navigatorIcon

        Researcher ->
            researcherIcon

        Sponsor ->
            sponsorIcon

        Mobster.RpgRole.Mobber ->
            mobberIcon


